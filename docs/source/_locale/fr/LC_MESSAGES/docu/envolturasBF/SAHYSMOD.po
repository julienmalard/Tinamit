msgstr ""

#: ../../../tinamit/EnvolturaBF/en/SAHYSMOD/SAHYSMOD_Wrapper.pydocstring of
#: tinamit.EnvolturaBF.en.SAHYSMOD.SAHYSMOD_Wrapper.read_output_file:5
msgid "The absolute path to the output file."
msgstr ""

