<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<config xmlns="http://zanata.org/namespace/config/">
    <url>https://translate.zanata.org</url>
    <project>tinamit</project>
    <project-version>2.0</project-version>
    <project-type>gettext</project-type>
    <src-dir>build/locale</src-dir>
    <trans-dir>source/_locale</trans-dir>
    <rules>
        <rule pattern="**/*.pot">{locale}/{path}/LC_MESSAGES/{filename}.po</rule>
    </rules>
</config>
